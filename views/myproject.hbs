<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Project</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9"
      crossorigin="anonymous"
    />
  </head>
  <body class="bg-body-secondary">
    <nav class="navbar navbar-expand-lg bg-white container-fluid">
      <div class="container">
        <a class="navbar-brand" href="#">
          <img
            src="images/logo_navbar.png"
            alt="Logo"
            style="width: 35px; height: auto"
          />
        </a>
        <div class="navbar-toggler-center">
          <button
            class="navbar-toggler"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>
        </div>

        <div
          class="collapse navbar-collapse text-center"
          id="navbarSupportedContent"
        >
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item mx-3">
              <a
                class="nav-link active"
                aria-current="page"
                href="index"
              >Home</a>
            </li>
            <li class="nav-item mx-3">
              <a class="nav-link fw-bolder text-danger" href="myproject">Add
                Project</a>
            </li>
            <li class="nav-item mx-3">
              <a class="nav-link" href="testimonial">My Testimonials</a>
            </li>
          </ul>
          <div
            class="d-flex collapse justify-content-center fw-bolder"
            role="search"
          >
            <a class="btn bg-black text-white" href="contact">Contact Me</a>
          </div>
        </div>
      </div>
    </nav>

    <!-- start project -->
    <div class="d-flex justify-content-center mt-5">
      <div>
        <div class="d-flex justify-content-center mb-5">
          <a href="formMyproject">
            <button
              class="rounded bg-danger text-white fw-bolder"
              style="border: none"
            >
              Add Project
            </button>
          </a>
        </div>

        <div>
          <div
            class="gap-5"
            style="display: flex; flex-wrap: wrap;  justify-content: center;"
          >
            {{#each dataProject}}
              <div
                class="card row-col-6 bg-transparent mb-5"
                style="width: 370px; height: auto; "
              >
                <div style="width: 100%; height: auto">
                  <div style="width: 100%; height: auto">
                    <img
                      src="images/anak.png"
                      class="card-img-top"
                      alt="my-project"
                      style="object-fit: cover; width: 100%; height: 250px"
                    />
                  </div>

                  <div class="card-body">
                    <a
                      href="projectDetail/:12345"
                      style="text-decoration: none;"
                    >
                      <h3 class="text-black">{{this.name}}</h3>
                    </a>

                    <h6>{{this.startDate}} - {{this.endDate}}</h6>

                    <h6>Durasi: {{this.Duration}}</h6>
                    <h6 class="card-text">{{this.description}}</h6>
                    <div class="project-icon">
                      <i
                        class="fa-brands fa-node-js"
                        style="margin-right: 8px"
                      ></i>
                      <i
                        class="fa-brands fa-square-js"
                        style="margin-right: 8px"
                      ></i>
                      <i
                        class="fa-brands fa-react"
                        style="margin-right: 8px"
                      ></i>
                      <i
                        class="fa-brands fa-vuejs"
                        style="margin-right: 8px"
                      ></i>
                    </div>
                    <div class="d-flex mt-2">
                      <button
                        class="w-50 rounded bg-black text-white"
                        style="border: none; margin-right: 8px"
                      >
                        edit
                      </button>
                      <button
                        class="w-50 rounded bg-black text-white"
                        style="border: none"
                      >
                        delete
                      </button>
                    </div>
                  </div>
                </div>
              </div>
            {{/each}}
          </div>
        </div>

      </div>
    </div>
    <!-- end project -->

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
      crossorigin="anonymous"
    ></script>
  </body>
</html>